#include <bits/stdc++.h>  //submitted by taleb
#define all(x) x.begin(), x.end()
#define allr(x) x.rbegin(), x.rend()
#define srt(x) sort(all(x))
#define ll long long
const ll mod =1e9+7;
const ll N = 5000005;
#define vp  vector<pair<ll,ll>>
#define vll  vector<ll>
#define pb push_back
#define mp make_pair
#define F first
#define S second
#define fr(i,a,b) for(int i = a; i <= b; i++)
#define rep(n) for(int i = 0; i < n; i++)
#define in(v)  for(auto &i : v)  cin >> i;
#define coutv(v) for(auto &i : v)  cout << i << ' ';
#define uni(a) a.erase(unique(all(a)),a.end());
#define nl '\n'
#define gcd(a,b)   __gcd(a,b);
#define lcm(a,b)  1LL*a*b/gcd(a,b);
#define mat  ios_base::sync_with_stdio(false); cin.tie(NULL);
#define yes  cout<<"YES"<<endl;
#define no  cout<<"NO"<<endl;
 #define ordered_set tree<int, null_type,less<int>, rb_tree_tag,tree_order_statistics_node_update>
#include<ext/pb_ds/assoc_container.hpp>
#include<ext/pb_ds/tree_policy.hpp>
using namespace __gnu_pbds;
using namespace std;
void solve()
{
 ll a,b,g; cin>>a>>b>>g;
 ll gd=gcd(g,1000000000-g);
 cout<<(1000000000-g)/gd<<"/"<<g/gd<<nl;
}
int main()     
{
  mat; 
  int tc=1; int t=1; 
    cin>>tc;
  while(tc--){
     cout<<"Case "<<t++<<": ";
    solve();
  }
}
