/*
1. Think Greedy
2. Think Brute Force
3. Think solution in reverse order
4. Think DP [ check constraints carefully ]
5. Check base case+s for DP and prove solution for Greedy
6. Think Graph
*/

void solve()
{
 string a,b; 
 cin>>a>>b;
 int la=a.size(),lb=b.size();
 for(int i=0;i+lb<=la;i++)
 {
  if(a.substr(i,lb)==b){ cout<<i<<endl; return;}
 }
} 
int main()
{
  fast();
  int tc=0; 
   cin>>tc;
  fr(i, 0, tc)
  {
    // cout << "Case " << i + 1 << ": ";
    solve();
  }
}
